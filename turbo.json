{
  "$schema": "https://turbo.build/schema.v2.json",
  "globalDependencies": [],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:e2e": {
      "cache": false,
      "dependsOn": ["^build"],
      "persistent": true
    },
    "dev:debug": {
      "cache": false,
      "persistent": true
    }
  }
}
