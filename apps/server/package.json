{
  "name": "server",
  "version": "0.0.15-beta",
  "packageManager": "npm@10.8.2",
  "scripts": {
    "cypress:open": "cypress open --config-file cypress.config.ts",
    "cypress:open:authentication": "cypress open --config-file cypress.config.authentication.ts",
    "cypress:open:nonProd": "cypress open --config-file cypress.config.nonProd.ts",
    "cypress:open:prod": "cypress open --config-file cypress.config.prod.ts",
    "cypress:open:syncing": "cypress open --config-file cypress.config.syncing.ts",
    "cypress:run": "cypress run --config-file cypress.config.ts",
    "cypress:run:authentication": "cypress run --config-file cypress.config.authentication.ts",
    "cypress:run:nonProd": "cypress run --config-file cypress.config.nonProd.ts",
    "cypress:run:prod": "cypress run --config-file cypress.config.prod.ts",
    "cypress:run:syncing": "cypress run --config-file cypress.config.syncing.ts",
    "elasticsearch": "docker volume create esdata && docker run --rm --name elasticsearch_container -p 9200:9200 -p 9300:9300 -v esdata:/usr/share/elasticsearch/data -e \"discovery.type=single-node\" -e \"xpack.security.enabled=false\" docker.elastic.co/elasticsearch/elasticsearch:7.10.2",
    "dev": "concurrently --kill-others \"npm run elasticsearch\" \"npm run server\" \"npm run redisserver\"",
    "dev:debug": "concurrently --kill-others \"npm run elasticsearch\" \"npm run server:debug\" \"npm run redisserver\"",
    "start:e2e": "nodemon",
    "redisserver": "docker run --rm --name ucw_redis -p 6379:6379 redis:7.2-alpine",
    "prod": "ENV=prod npm run dev",
    "server": "wait-on http://localhost:9200 && nodemon",
    "server:debug": "wait-on http://localhost:9200 && node --inspect -r ts-node/register ./src/server.js",
    "start": "npx ts-node --transpile-only src/server.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "coverage": "npm test -- --verbose --coverage --watchAll",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --no-coverage",
    "test:watch": "cross-env DEBUG_PRINT_LIMIT=50000 npm run test -- --watch",
    "test-clear": "npm test -- --clearCache"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@capacitor-community/http": "^1.4.1",
    "@ngrok/ngrok": "^1.5.0",
    "@opensearch-project/opensearch": "^2.13.0",
    "@repo/akoya-adapter": "*",
    "@repo/finicity-adapter": "*",
    "@repo/flinks-adapter": "*",
    "@repo/mx-adapter": "*",
    "@repo/sophtron-adapter": "*",
    "@repo/utils": "*",
    "@short.io/opensearch-mock": "^0.4.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/he": "^1.2.3",
    "@types/uuid": "^9.0.8",
    "assert-browserify": "^2.0.0",
    "axios": "^1.6.8",
    "buffer-browserify": "^0.2.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-rate-limit": "^7.0.2",
    "he": "^1.2.0",
    "joi": "^17.13.3",
    "js-logger": "^1.6.1",
    "jsonwebtoken": "^9.0.2",
    "mx-platform-node": "^1.8.2",
    "prettier": "^3.4.2",
    "redis": "^4.6.12",
    "sanitize-filename": "^1.6.3",
    "set-interval-async": "^3.0.3",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.18.6",
    "@repo/utils-dev-dependency": "*",
    "@repo/utils-e2e": "*",
    "@testing-library/cypress": "^10.0.1",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "babel-plugin-js-logger": "^1.0.17",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "cypress": "^13.7.1",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^30.1.3",
    "jest-mock-extended": "^4.0.0-beta1",
    "msw": "^2.2.13",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "wait-on": "^8.0.1"
  }
}
